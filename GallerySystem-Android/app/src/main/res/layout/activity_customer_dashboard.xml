<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CustomerDashboardActivity">


    <Button
        android:id="@+id/goShoppingCart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go to Shopping Cart"
        android:onClick="customerGoShoppingCart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/customerLogOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="Log Out"
        android:onClick="customerLogout"
        app:layout_constraintEnd_toEndOf="@+id/goShoppingCart"
        app:layout_constraintStart_toStartOf="@+id/goShoppingCart"
        app:layout_constraintTop_toBottomOf="@+id/goShoppingCart" />
</androidx.constraintlayout.widget.ConstraintLayout>